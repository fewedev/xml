{
    "global": {
        "systemName": "fewedev_xml"
    },
    "system": {
        "serverNames": [
            "cli"
        ]
    },
    "cli": {
        "imageName": "fewedev/cli",
        "imageTag": "8.3",
        "containerVolumes": [
            "${systemPath}:/var/www/fewedev/xml:me:w"
        ],
        "containerPrepareDockerScript": "ads/cli/docker/prepare.sh",
        "containerInstallDockerScript": "ads/cli/docker/install.sh",
        "bashPath": "/var/www/fewedev/xml",
        "bashUser": "me"
    }
}
